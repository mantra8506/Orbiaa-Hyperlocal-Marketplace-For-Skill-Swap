<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rgjzrwwe7ger"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/cr45bfff7d4"
        android:paddingTop="105dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/r3ut66k40z8w"
                android:textColor="#000000"
                android:textSize="30sp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="10dp"
                android:layout_marginHorizontal="22dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:text="Upload your Pictures"
                />
            <TextView
                android:id="@+id/rb7hw9zvhx3"
                android:textColor="#000000"
                android:textSize="13sp"
                android:layout_marginBottom="15dp"
                android:layout_marginHorizontal="25dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:text="Personalize your account with a profile picture upload"
                />
            <LinearLayout
                android:id="@+id/r1mgo98tjs9k"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/s000000sw2cr10bfbeaa0"
                android:paddingVertical="27dp"
                android:paddingHorizontal="14dp"
                android:layout_marginBottom="10dp"
                android:layout_marginHorizontal="22dp"
                android:elevation="4dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rhiif6qyfpvb"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cr10bb79552"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="29dp"
                    android:elevation="4dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/rhd1jxss1vin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cr10b4c3d3d"
                        android:paddingVertical="36dp"
                        android:layout_marginTop="13dp"
                        android:layout_marginBottom="13dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/rnvmyp00rdhr"
                            android:layout_width="match_parent"
                            android:layout_height="80dp"
                            android:layout_marginBottom="12dp"
                            android:layout_marginHorizontal="90dp"
                            android:src="@drawable/upload"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/rhmxhwga1l15"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:layout_marginHorizontal="80dp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:textAlignment="center"
                            android:text="Upload Picture"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ret8ym11j1u"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/cr10bb79552"
                    android:paddingHorizontal="17dp"
                    android:elevation="4dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/ry03fxmaf81"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/sd9d9d9sw1cr10"
                        android:paddingHorizontal="125dp"
                        android:layout_marginTop="9dp"
                        android:layout_marginBottom="9dp"
                        android:elevation="4dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/r3hxzlh2cdzq"
                            android:layout_gravity="center"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginHorizontal="8dp"
                            android:src="@drawable/upload"
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <ImageView
                android:id="@+id/r7d31ldtczfe"
                android:layout_gravity="center"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginHorizontal="8dp"
                android:src="@drawable/warning"
                />
            <TextView
                android:id="@+id/rmzd48n32fgj"
                android:textColor="#000000"
                android:textSize="15sp"
                android:layout_marginBottom="25dp"
                android:layout_marginHorizontal="66dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:text="Please upload a minimum of 1 picture from the available 2"
                />
            <LinearLayout
                android:id="@+id/r0eqj1377z3th"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginHorizontal="22dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/reft4jz8cl7"
                    android:layout_width="34dp"
                    android:layout_height="34dp"
                    android:src="@drawable/baseline_arrow_back_24"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <LinearLayout
                    android:id="@+id/r2eo8chrg8mq"
                    android:layout_width="132dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/s8e8383sw1cr10b4c3d3d"
                    android:paddingVertical="19dp"
                    android:elevation="4dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/ruqnh4m03og"
                        android:textColor="#FFFFFF"
                        android:textSize="19sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Done "
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>